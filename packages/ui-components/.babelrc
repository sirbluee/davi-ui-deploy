{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react", // For React JSX support
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript" // For TypeScript support
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
